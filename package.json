{
  "name": "in-gfw",
  "version": "1.2.0",
  "description": "Identify current location is located in mainland China.",
  "main": "lib/index.js",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  },
  "babel": {
    "presets": [
      [
        "@babel/env"
      ]
    ],
    "plugins": [
      "@babel/transform-runtime"
    ]
  },
  "scripts": {
    "report-coverage": "codecov",
    "pretest": "eslint lib test",
    "test": "node . && nyc mocha --no-timeouts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gucong3000/in-gfw.git"
  },
  "keywords": [
    "OS",
    "system",
    "location",
    "geo",
    "PRC",
    "mainland",
    "China",
    "gfw",
    "Great",
    "Firewall"
  ],
  "author": "gucong3000",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gucong3000/in-gfw/issues"
  },
  "homepage": "https://github.com/gucong3000/in-gfw#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-beta.51",
    "ci-info": "^1.1.3",
    "codecov": "^3.0.2",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "mock-fs": "^4.5.0",
    "nyc": "^12.0.2",
    "proxyquire": "^2.0.1",
    "util-promisify": "^2.1.0"
  },
  "dependencies": {
    "glob": "^7.1.2",
    "is-wsl": "^1.1.0",
    "mem": "^3.0.1"
  }
}
