{
  "name": "in-gfw",
  "version": "1.0.0",
  "description": "Identify current location is located in mainland China.",
  "main": "lib/index.js",
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true
  },
  "babel": {
    "presets": [
      [
        "env"
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "report-coverage": "codecov",
    "pretest": "eslint *.js lib test",
    "test": "node . && nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gucong3000/in-gfw.git"
  },
  "keywords": [
    "OS",
    "system",
    "location",
    "geo",
    "PRC",
    "mainland",
    "China",
    "gfw",
    "Great",
    "Firewall"
  ],
  "author": "gucong3000",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gucong3000/in-gfw/issues"
  },
  "homepage": "https://github.com/gucong3000/in-gfw#readme",
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "ci-info": "^1.1.3",
    "codecov": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.5",
    "mock-fs": "^4.4.2",
    "nyc": "^11.6.0",
    "proxyquire": "^2.0.1",
    "util-promisify": "^2.1.0"
  },
  "dependencies": {
    "glob": "^7.1.2",
    "is-wsl": "^1.1.0",
    "mem": "^3.0.0"
  }
}
